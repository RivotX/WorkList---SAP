<mvc:View
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.uxap"
    xmlns:layout="sap.ui.layout"
    xmlns:m="sap.m"
    controllerName="com.myorg.myapp.controller.Detail"
    height="100%"
    xmlns:l="sap.ui.layout"
    xmlns:f="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
>
    <!-- <m:Page > -->
    <ObjectPageLayout
        id="ObjectPageLayout"
        enableLazyLoading="true"
        beforeNavigate="onBeforeNavigate"
        useIconTabBar="true"
        showTitleInHeaderContent="true"
        upperCaseAnchorBar="false"
        sectionChange="onSectionChange"
        class="DetailBackgroundColor"
    >
        <headerTitle>
            <ObjectPageDynamicHeaderTitle>
                <expandedHeading>
                    <m:Title
                        text="{i18n>Package}"
                        wrapping="true"
                    />
                </expandedHeading>

                <snappedHeading>
                    <m:FlexBox
                        fitContainer="true"
                        alignItems="Center"
                    >
                        <m:Title
                            text="{i18n>Package}"
                            wrapping="true"
                        />
                    </m:FlexBox>
                </snappedHeading>
            </ObjectPageDynamicHeaderTitle>
        </headerTitle>

        <headerContent>
            <m:VBox class="sapUiSmallMargin">
                <f:Form
                    id="FormChangeColumn_threeGroups234"
                    editable="false"
                >
                    <f:title>
                        <core:Title text="Supplier" />
                    </f:title>
                    <f:layout>
                        <f:ColumnLayout
                            columnsM="2"
                            columnsL="3"
                            columnsXL="4"
                        />
                    </f:layout>
                    <f:formContainers>
                        <f:FormContainer title="Address">
                            <f:formElements>
                                <f:FormElement label="Name">
                                    <f:fields>
                                        <m:Input
                                            value="s"
                                            id="name"
                                            editable="false"
                                        />
                                    </f:fields>
                                </f:FormElement>
                                <f:FormElement label="Street">
                                    <f:fields>
                                        <m:Input value="{Street}" />
                                    </f:fields>
                                </f:FormElement>
                                <f:FormElement label="Street">
                                    <f:fields>
                                        <m:Input value="{Street}" />
                                    </f:fields>
                                </f:FormElement>
                            </f:formElements>
                        </f:FormContainer>
                        <f:FormContainer title="Contact">
                            <f:formElements>
                                <f:FormElement label="Twitter">
                                    <f:fields>
                                        <m:Input value="{Twitter}" />
                                    </f:fields>
                                </f:FormElement>
                                <f:FormElement label="Email">
                                    <f:fields>
                                        <m:Input
                                            value="{Email}"
                                            type="Email"
                                        />
                                    </f:fields>
                                </f:FormElement>
                                <f:FormElement label="Tel.">
                                    <f:fields>
                                        <m:Input
                                            value="{Tel}"
                                            type="Tel"
                                        />
                                    </f:fields>
                                </f:FormElement>
                            </f:formElements>
                        </f:FormContainer>
                        <f:FormContainer title="Other">
                            <f:formElements>
                                <f:FormElement label="SMS">
                                    <f:fields>
                                        <m:Input
                                            value="{Sms}"
                                            type="Tel"
                                        />
                                    </f:fields>
                                </f:FormElement>
                                <f:FormElement label="Mobile">
                                    <f:fields>
                                        <m:Input
                                            value="{Mobile}"
                                            type="Tel"
                                        />
                                    </f:fields>
                                </f:FormElement>
                                <f:FormElement label="Pager">
                                    <f:fields>
                                        <m:Input
                                            value="{Pager}"
                                            type="Tel"
                                        />
                                    </f:fields>
                                </f:FormElement>
                            </f:formElements>
                        </f:FormContainer>
                    </f:formContainers>
                </f:Form>
            </m:VBox>
