<mvc:View
    xmlns:table="sap.ui.table"
    controllerName="com.myorg.myapp.controller.Table"
    displayBlock="true"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    core:require="{
        formatter: 'com/myorg/myapp/model/formatter'
    }"
>
    <table:Table
        id="sampleTable"
        rows="{ path:'data>/results'}"
        class="myTable"
    >
        <table:columns>
            <table:Column>
                <Label text='' />
                <table:template>
                    <core:Icon src="sap-icon://product" />
                </table:template>
            </table:Column>

            <table:Column>
                <Label text='{i18n>1Col}' />
                <table:template>
                    <Text text="{data>OBJID}" />
                </table:template>
            </table:Column>

            <table:Column>
                <Label text='{i18n>2Col}' />
                <table:template>
                    <Text text="{data>PACKAGE_NAME}" />
                </table:template>
            </table:Column>

            <table:Column>
                <Label text='{i18n>3Col}' />
                <table:template>
                    <Text text="{data>DESCRIPTION}" />
                </table:template>
            </table:Column>

            <table:Column>
                <Label text='{i18n>4Col}' />
                <table:template>
                    <!-- <Text text="{data>STATUS}" />  #6cd331-->
                    <core:Icon
                        src="sap-icon://circle-task-2"
                        
                    />
                </table:template>
            </table:Column>

            <table:Column>
                <Label text='{i18n>5Col}' />
                <table:template>
                    <Text
                        text="{
                        path: 'data>CREATION_DATE',
                        formatter: '.formatter.DateFormat'
                    }"
                    />
                </table:template>
            </table:Column>

            <table:Column>
                <Label text='{i18n>6Col}' />
                <table:template>
                    <Text text="??????" />
                </table:template>
            </table:Column>

            <table:Column>
                <Label text='{i18n>7Col}' />
                <table:template>
                    <Text
                        text="{
                path: 'data>data>VARIABLE_01',
                formatter: '.formatter.NumberFormat2'
                }"
                    />
                </table:template>
            </table:Column>

            <table:Column>
                <Label text='{i18n>8Col}' />
                <table:template>
                    <Text
                        text="{
                path: 'data>data>VARIABLE_02',
                formatter: '.formatter.NumberFormatt'
                }"
                    />
                </table:template>
            </table:Column>

            <table:Column>
                <Label text='{i18n>type}' />
                <table:template>
                    <Text
                        text="{
                    path: 'data>data>TYPE_PACKAGE',
                    formatter: '.formatter.TypeFormat'
                    }"
                    />
                </table:template>
            </table:Column>

            <table:Column>
                <Label text='{i18n>Priority}' />
                <table:template>
                    <core:Icon
                        color="#6cd331"
                        src="{path: 'data>PRIORITY',
                         formatter: '.formatter.PriorityFormat'}"
                        class="iconoCentro"
                    />
                </table:template>
            </table:Column>
        </table:columns>
    </table:Table>
</mvc:View>
